{
  "name": "swallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "android:local": "ENVFILE=.env.local.android react-native run-android",
    "ios": "ENVFILE=.env react-native run-ios",
    "ios:local": "ENVFILE=.env.local react-native run-ios",
    "start": "ENVFILE=.env react-native start",
    "ganache": "npx ganache-cli",
    "test": "ENVFILE=.env.test jest --verbose --testTimeout=60000",
    "test:watch": "ENVFILE=.env.test jest --verbose --watch --coverage --testTimeout=60000",
    "test:ci": "jest --coverage --verbose --testTimeout=60000",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "postinstall": "rn-nodeify --install buffer,events,stream,crypto --hack --yarn"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.4.0",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/stack": "^6.0.9",
    "@rsksmart/rif-id-ethr-did": "^0.1.0",
    "@rsksmart/rif-id-mnemonic": "^0.1.1",
    "@rsksmart/rlogin-dpath": "^1.0.1",
    "@rsksmart/rns-resolver.js": "^1.0.1",
    "@rsksmart/rns-sdk": "^1.0.0-beta.3",
    "@rsksmart/rsk-contract-metadata": "^1.0.15",
    "@rsksmart/rsk-testnet-contract-metadata": "^1.0.11",
    "@rsksmart/rsk-utils": "^1.1.0",
    "@walletconnect/client": "^1.6.6",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "buffer": "^4.9.2",
    "ethers": "^5.4.7",
    "events": "^1.1.1",
    "i18next": "^21.4.1",
    "process": "^0.11.10",
    "react": "17.0.2",
    "react-i18next": "^11.13.0",
    "react-native": "0.65.1",
    "react-native-camera": "^4.2.1",
    "react-native-config": "^1.4.5",
    "react-native-crypto": "^2.1.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.7.0",
    "react-native-randombytes": "^3.0.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-svg": "^12.1.1",
    "react-native-uuid": "^2.0.1",
    "react-native-webview": "^11.14.3",
    "react-qr-code": "^2.0.2",
    "readable-stream": "1.0.33",
    "rn-secure-storage": "^2.0.7",
    "socket.io-client": "^4.4.1",
    "stream-browserify": "^1.0.0",
    "url-parse": "^1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@rsksmart/rlogin-eip1193-types": "^1.0.0-beta.7",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.65.0",
    "@types/react-test-renderer": "^17.0.1",
    "@types/readable-stream": "^2.3.12",
    "@types/url-parse": "^1.4.5",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "ganache-cli": "^6.12.2",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "rn-nodeify": "^10.3.0",
    "typescript": "^3.8.3",
    "wait-for-expect": "^3.0.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "./testLib/setupTests.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native-linear-gradient)/"
    ]
  },
  "react-native": {
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "crypto": "react-native-crypto"
  },
  "browser": {
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "crypto": "react-native-crypto"
  }
}
