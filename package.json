  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "android:local": "ENVFILE=.env.local.android react-native run-android",
    "android:build": "ENVFILE=.env npx react-native-clean-project && cd android && ./gradlew app:assembleRelease",
    "ios": "ENVFILE=.env react-native run-ios",
    "ios:local": "ENVFILE=.env.local react-native run-ios",
    "ios:small": "ENVFILE=.env npx react-native run-ios --simulator=\"iPhone 8\"",
    "start": "ENVFILE=.env react-native start",
    "ganache": "npx ganache-cli",
    "test": "ENVFILE=.env.test jest --verbose --testTimeout=60000",
    "test:watch": "ENVFILE=.env.test jest --verbose --watch --coverage --testTimeout=60000",
    "test:ci": "jest --coverage --verbose --testTimeout=60000",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "postinstall": "rn-nodeify --install buffer,events,stream,crypto --hack --yarn",
    "clean:android": "cd android && ./gradlew clean && ./gradlew cleanBuildCache && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && pod cache clean --all && pod deintegrate && pod setup && pod install && cd .."
  },
